SELECT
        name,
        surname,
        subject,
        grade
FROM Students
WHERE grade>65;


explain analyze SELECT
        name,
        surname,
        subject,
        grade
FROM Students
WHERE grade>65;


create index grade_index on Student(grade);


SELECT
        name,
        surname,
        subject,
        grade
FROM Students
WHERE grade>65;


explain analyze SELECT
        name,
        surname,
        subject,
        grade
FROM Students
WHERE grade>65;

DROP INDEX grade_index;
