longest_string = 0
for string in range(5):
    text = input("enter text: ")
    if longest_string < len(text):
        longest_string = len(text)
print("the longest strings lenght is", longest_string)




def max_number(list):
    max = list[0]
    for number in list:
        if number > max:
            max = number
    return max


def factorial(n):
    result = 1
    for number in range(1, n+1):
        result = result * number
    return result
